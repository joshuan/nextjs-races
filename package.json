{
  "name": "@joshuan/races",
  "description": "Formula races calendar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:calendar && npm run build:next",
    "build:schema": "typescript-json-schema ./@types/eventFile.ts IEventFile > @types/schema.json",
    "build:ergast": "ts-node calendar/commands/ergast.ts",
    "build:calendar": "ts-node calendar/commands/build.ts",
    "build:next": "next build",
    "start": "next start",
    "ergast": "YEAR=2022 run-s ergast:prepare ergast:download ergast:extract ergast:convert ergast:clean",
    "ergast:prepare": "mkdir -p ./data/ergast/$YEAR",
    "ergast:download": "curl http://ergast.com/downloads/f1db_csv.zip -o ./data/ergast/$YEAR/f1db_csv.zip",
    "ergast:extract": "unzip -o ./data/ergast/$YEAR/f1db_csv.zip -d ./data/ergast/$YEAR",
    "ergast:convert": "ts-node commands/ergast-csv-to-json.ts",
    "ergast:clean": "rm -rf ./data/ergast/$YEAR/*.zip ./data/ergast/$YEAR/*.csv"
  },
  "dependencies": {
    "moment": "2.29.1",
    "next": "^11.0.0",
    "next-ym": "2.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.3",
    "@types/pify": "5.0.1",
    "@types/react": "17.0.24",
    "axios": "0.27.2",
    "csv-parse": "5.0.4",
    "ergast-client": "0.2.9",
    "fastest-validator": "1.11.1",
    "geo-tz": "6.0.1",
    "ical-generator": "3.0.0",
    "js-yaml": "4.1.0",
    "npm-run-all": "4.1.5",
    "pify": "5.0.0",
    "ts-node": "10.2.1",
    "typescript": "4.4.3",
    "typescript-json-schema": "0.51.0",
    "ydb-sdk": "2.5.0"
  }
}
